<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="motorcycle_list_action" model="ir.actions.act_window">
            
            <field name="name">Motorcycle List</field>
            <field name="res_model">motorcycle.registry</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create the first Registry.
                </p>
            </field>
    </record>

    <menuitem name="Motorcycle Registry" id="menu_motorcycle" sequence="1" web_icon="motorcycle,static/description/icon.png">
        <menuitem id="menu_academy_sub" name="Motorcycle Registry" sequence="1">
            <menuitem id="menu_motorcycle_registry" name="Registry" sequence="2" action="motorcycle_list_action"/>
        </menuitem>
    </menuitem>  

</odoo>